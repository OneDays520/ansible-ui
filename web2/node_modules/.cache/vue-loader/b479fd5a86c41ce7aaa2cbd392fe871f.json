      text=\"Are you really want to delete this inventory?\"\n      v-model=\"deleteItemDialog\"\n      @yes=\"deleteItem(itemId)\"\n    />\n\n    <v-toolbar flat color=\"white\">\n      <v-app-bar-nav-icon @click=\"showDrawer()\"></v-app-bar-nav-icon>\n      <v-toolbar-title>主机清单</v-toolbar-title>\n      <v-spacer></v-spacer>\n      <v-btn\n        color=\"primary\"\n        @click=\"editItem('new')\"\n      >新建清单</v-btn>\n    </v-toolbar>\n\n    <v-data-table\n      :headers=\"headers\"\n      :items=\"items\"\n      hide-default-footer\n      class=\"mt-4\"\n      :items-per-page=\"Number.MAX_VALUE\"\n    >\n      <template v-slot:item.type=\"{ item }\">\n        <code>{{ item.type }}</code>\n      </template>\n      <template v-slot:item.inventory=\"{ item }\">\n        {{ item.type === 'file' ? item.inventory : '&mdash;' }}\n      </template>\n      <template v-slot:item.actions=\"{ item }\">\n        <div style=\"white-space: nowrap\">\n          <v-btn\n            icon\n            class=\"mr-1\"\n            @click=\"askDeleteItem(item.id)\"\n          >\n            <v-icon>mdi-delete</v-icon>\n          </v-btn>\n\n          <v-btn\n            icon\n            class=\"mr-1\"\n            @click=\"editItem(item.id)\"\n          >\n            <v-icon>mdi-pencil</v-icon>\n          </v-btn>\n        </div>\n      </template>\n    </v-data-table>\n  </div>\n\n</template>\n<script>\nimport ItemListPageBase from '@/components/ItemListPageBase';\nimport InventoryForm from '@/components/InventoryForm.vue';\n\nexport default {\n  mixins: [ItemListPageBase],\n  components: { InventoryForm },\n  methods: {\n    getHeaders() {\n      return [{\n        text: '名称',\n        value: 'name',\n        width: '33.33%',\n      },\n      {\n        text: '类型',\n        value: 'type',\n        width: '33.33%',\n      },\n      {\n        text: '路径',\n        value: 'inventory',\n        width: '33.33%',\n      },\n      {\n        text: '动作',\n        value: 'actions',\n        sortable: false,\n      }];\n    },\n    getItemsUrl() {\n      return `/api/project/${this.projectId}/inventory`;\n    },\n    getSingleItemUrl() {\n      return `/api/project/${this.projectId}/inventory/${this.itemId}`;\n    },\n    getEventName() {\n      return 'i-inventory';\n    },\n  },\n};\n</script>\n"]}]}