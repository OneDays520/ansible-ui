A;AAAA,OAFA,CAZA;AAgBA,MAAA,aAAA,EAAA,CACA,UAAA,CAAA;AAAA,eAAA,CAAA,CAAA,CAAA,IAAA,sBAAA;AAAA,OADA;AAhBA,KAAA;AAoBA,GAtBA;AAwBA,EAAA,OAxBA,qBAwBA;AAAA;;AAAA;AAAA;AAAA;AAAA;AAAA;AACA,kBAAA,KAAA,CAAA,eAAA,EAAA,EAAA;AACA,gBAAA,QAAA,CAAA,QAAA,GAAA,QAAA,CAAA,OAAA;AACA;;AAHA;AAAA;AAAA;AAAA;AAAA;AAAA;AAAA;AAIA,GA5BA;AA8BA,EAAA,OAAA,EAAA;AACA,IAAA,eADA,6BACA;AACA,aAAA,QAAA,CAAA,MAAA,CAAA,QAAA,CAAA,YAAA,CAAA;AACA,KAHA;AAKA,IAAA,MALA,oBAKA;AAAA;;AAAA;AAAA;AAAA;AAAA;AAAA;AACA,gBAAA,MAAA,CAAA,WAAA,GAAA,IAAA;;AADA,oBAGA,MAAA,CAAA,KAAA,CAAA,UAAA,CAAA,QAAA,EAHA;AAAA;AAAA;AAAA;;AAAA;;AAAA;AAOA,gBAAA,MAAA,CAAA,aAAA,GAAA,IAAA;AAPA;AAAA;AAAA,uBASA,KAAA,CAAA;AACA,kBAAA,MAAA,EAAA,MADA;AAEA,kBAAA,GAAA,EAAA,iBAFA;AAGA,kBAAA,YAAA,EAAA,MAHA;AAIA,kBAAA,IAAA,EAAA;AACA,oBAAA,IAAA,EAAA,MAAA,CAAA,QADA;AAEA,oBAAA,QAAA,EAAA,MAAA,CAAA;AAFA;AAJA,iBAAA,CATA;;AAAA;AAmBA,gBAAA,QAAA,CAAA,QAAA,GAAA,QAAA,CAAA,OAAA;AAnBA;AAAA;;AAAA;AAAA;AAAA;AAqBA,gBAAA,OAAA,CAAA,GAAA;;AACA,oBAAA,aAAA,QAAA,CAAA,MAAA,KAAA,GAAA,EAAA;AACA,kBAAA,MAAA,CAAA,WAAA,GAAA,6BAAA;AACA,iBAFA,MAEA;AACA,kBAAA,MAAA,CAAA,WAAA,GAAA,eAAA,cAAA;AACA;;AA1BA;AAAA;AA4BA,gBAAA,MAAA,CAAA,aAAA,GAAA,KAAA;AA5BA;;AAAA;AAAA;AAAA;AAAA;AAAA;AAAA;AAAA;AA8BA;AAnCA;AA9BA,CAAA","sourcesContent":["<template>\n  <div class=\"auth\">\n    <v-container\n      fluid\n      fill-height\n      align-center\n      justify-center\n      class=\"pa-0\"\n    >\n      <v-form\n        ref=\"signInForm\"\n        lazy-validation\n        v-model=\"signInFormValid\"\n        style=\"width: 300px; height: 300px;\"\n      >\n        <h3 class=\"text-center mb-8\">JY-SEMAPHORE</h3>\n\n        <v-alert\n          :value=\"signInError != null\"\n          color=\"error\"\n          style=\"margin-bottom: 20px;\"\n        >{{ signInError }}</v-alert>\n\n        <v-text-field\n          v-model=\"username\"\n          label=\"账户\"\n          :rules=\"usernameRules\"\n          autofocus\n          required\n          :disabled=\"signInProcess\"\n        ></v-text-field>\n\n        <v-text-field\n          v-model=\"password\"\n          label=\"密码\"\n          :rules=\"[v => !!v || 'Password is required']\"\n          type=\"password\"\n          required\n          :disabled=\"signInProcess\"\n          @keyup.enter.native=\"signIn\"\n          style=\"margin-bottom: 20px;\"\n        ></v-text-field>\n        <v-btn\n          color=\"primary\"\n          @click=\"signIn\"\n          :disabled=\"signInProcess\"\n          block\n        >\n          登录\n        </v-btn>\n      </v-form>\n    </v-container>\n  </div>\n</template>\n<style lang=\"scss\">\n.auth {\n  height: 100vh;\n}\n</style>\n<script>\nimport axios from 'axios';\nimport { getErrorMessage } from '@/lib/error';\n\nexport default {\n  data() {\n    return {\n      signInFormValid: false,\n      signInError: null,\n      signInProcess: false,\n\n      password: '',\n      username: '',\n      email: '',\n\n      emailRules: [\n        (v) => !!v || 'Email is required',\n      ],\n      passwordRules: [\n        (v) => !!v || 'Password is required',\n        (v) => v.length >= 6 || 'Password too short. Min 6 characters',\n      ],\n      usernameRules: [\n        (v) => !!v || 'Username is required',\n      ],\n    };\n  },\n\n  async created() {\n    if (this.isAuthenticated()) {\n      document.location = document.baseURI;\n    }\n  },\n\n  methods: {\n    isAuthenticated() {\n      return document.cookie.includes('semaphore=');\n    },\n\n    async signIn() {\n      this.signInError = null;\n\n      if (!this.$refs.signInForm.validate()) {\n        return;\n      }\n\n      this.signInProcess = true;\n      try {\n        await axios({\n          method: 'post',\n          url: '/api/auth/login',\n          responseType: 'json',\n          data: {\n            auth: this.username,\n            password: this.password,\n          },\n        });\n\n        document.location = document.baseURI;\n      } catch (err) {\n        console.log(err);\n        if (err.response.status === 401) {\n          this.signInError = 'Incorrect login or password';\n        } else {\n          this.signInError = getErrorMessage(err);\n        }\n      } finally {\n        this.signInProcess = false;\n      }\n    },\n  },\n};\n</script>\n"],"sourceRoot":"src/views"}]}